{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0.2",
    "ext-intl": "*",
    "asantibanez/livewire-select": "^2.1",
    "backpack/crud": "^5.0",
    "beberlei/assert": "^3.3",
    "cknow/laravel-money": "^6.3",
    "fruitcake/laravel-cors": "^2.0.5",
    "guzzlehttp/guzzle": "^7.2",
    "intervention/image": "^2.7",
    "laravel/framework": "^9.0",
    "laravel/tinker": "^2.7",
    "livewire/livewire": "^2.8",
    "mediconesystems/livewire-datatables": "^0.7.0",
    "protonemedia/laravel-form-components": "^3.5",
    "pusher/pusher-php-server": "^7.0",
    "spatie/data-transfer-object": "^3.7",
    "spatie/laravel-enum": "^3.0.0",
    "spatie/laravel-medialibrary": "^10.0.0",
    "spatie/laravel-translatable": "^5.2",
    "ziming/json-field-for-backpack": "^1.1.2"
  },
  "require-dev": {
    "backpack/generators": "^3.1",
    "barryvdh/laravel-debugbar": "^3.6",
    "barryvdh/laravel-ide-helper": "^2.12",
    "spatie/laravel-ignition": "^1.0",
    "fakerphp/faker": "^1.9.2",
    "laracasts/generators": "^2.0",
    "laravel/breeze": "^1.4",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^6.1",
    "phpunit/phpunit": "^9.5.10"
  },
  "provide": {
    "psr/container-implementation": "1.1|2.0",
    "psr/simple-cache-implementation": "1.0|2.0|3.0"
  },
  "conflict": {
    "tightenco/collect": "<5.5.33"
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Factories\\Glossary\\": "database/factories/Glossary",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "barryvdh/laravel-ide-helper"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
